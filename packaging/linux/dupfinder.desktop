[Desktop Entry]
Version=1.0
Type=Application
Name=CloneClean
GenericName=Duplicate File Finder
Comment=Find and remove duplicate files on your system
Exec=cloneclean %F
Icon=cloneclean
Terminal=false
Categories=Utility;FileTools;Qt;
MimeType=inode/directory;
Keywords=duplicate;finder;cleanup;files;hash;
StartupNotify=true
StartupWMClass=cloneclean

[Desktop Action Scan]
Name=Scan for Duplicates
Exec=cloneclean --scan %F

[Desktop Action QuickScan]
Name=Quick Scan
Exec=cloneclean --quick-scan %F

Actions=Scan;QuickScan;
